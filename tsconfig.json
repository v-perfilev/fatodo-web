{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "esnext",
    "target": "esnext",
    "lib": ["es2019", "dom"],
    "jsx": "preserve",
    "sourceMap": true,
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": true,
    "removeComments": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true
  }
}
